    }
    { URLLabelGambasOne URLLabel
      MoveScaled(63,0,14,3)
      AutoResize = True
      Text = ("Gambas tips")
      Alignment = Align.Center
      Link = "https://wordpress.gambas.one/"
    }
    { Panel2 Panel
      MoveScaled(78,1,2,2)
    }
    { URLLabelGambasOneForum URLLabel
      MoveScaled(81,0,14,3)
      AutoResize = True
      Text = ("Gambas forum")
      Alignment = Align.Center
      Link = "https://forum.gambas.one/"
    }
    { Panel1 Panel
