      MoveScaled(16,0,21,4)
      Enabled = False
      Expand = True
    }
    { CheckBoxAll CheckBox
      MoveScaled(37,0,25,3)
      AutoResize = True
      Text = ("Show all programs")
    }
  }
  { LabelMessage Label
    MoveScaled(7,19,42,3)
    Font = Font["Bold"]
    Alignment = Align.Center
  }
  { GridViewDisplay GridView
    MoveScaled(23,30,16,16)
    Expand = True
  }
  { HBox2 HBox
    MoveScaled(5,67,57,4)
    { ButtonSelect Button
      MoveScaled(1,0,13,4)
      Enabled = False
      Text = Shortcut(("Select"), "S")
    }
    { Panel1 Panel
      MoveScaled(19,0.4286,1,3)
    }
    { LabelSelected Label
      MoveScaled(24,0,24,4)
    }
  }
}
